{
  "name": "vscode-ember-template-lint",
  "displayName": "(Unofficial) Ember template lint for VS Code",
  "version": "0.0.1",
  "description": "Lint your templates as you edit them",
  "categories": [
    "Other"
  ],
  "keywords": [
    "ember",
    "template",
    "hbs",
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/grconrad/vscode-ember-template-lint.git"
  },
  "license": "MIT",
  "publisher": "grconrad",
  "main": "./out/extension.js",
  "files": [
    "out",
    "node_modules"
  ],
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js",
    "vscode:prepublish": "yarn run compile",
    "watch": "tsc -watch -p ./"
  },
  "activationEvents": [
    "workspaceContains:.template-lintrc.js"
  ],
  "dependencies": {
    "execa": "^4.0.0",
    "find-up": "^4.1.0"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.44.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "sort-package-json": "^1.42.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "engines": {
    "vscode": "^1.44.0"
  },
  "volta": {
    "node": "12.13.0",
    "yarn": "1.19.1"
  }
}
